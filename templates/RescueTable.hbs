<table class="full-width rescues padded striped">
  <thead>
    <td class="system">System</td>

    <td class="state">State</td>

    <td class="codeRed">Code Red</td>

    <td class="rats">Rats</td>

    <td class="date">Date</td>
  </thead>

  <tbody></tbody>
</table>

<menu type="toolbar">
  <div class="primary">
    {{#if nextPage}}
      <button class="next" type="button">Older</a>
    {{/if}}
  </div>

  <div class="page">
    Page {{currentPage}} of {{lastPage}}
  </div>

  <div class="secondary">
    {{#if previousPage}}
      <button class="previous" type="button">Newer</a>
    {{/if}}
  </div>
</menu>
