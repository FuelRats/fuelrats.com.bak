<header class="page-header">
  <h1>Users</h1>
</header>

<table class="user-list padded striped">
  <thead>
    <th>Avatar</th>

    <th>Email</th>

    <th>CMDRs</th>

    <th>Nicknames</th>
  </thead>

  <tbody></tbody>
</table>

<menu type="toolbar">
  <div class="primary">
    {{#if nextPage}}
      <a class="button" href="/users/page/{{nextPage}}" id="next" type="button">Older</a>
    {{/if}}
  </div>
  <div class="secondary">
    {{#if previousPage}}
      <a class="button" href="/users/page/{{previousPage}}" id="previous" type="button">Newer</a>
    {{/if}}
  </div>
</menu>
